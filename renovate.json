{
  "enabled": true,
  "timezone": "Europe/Berlin",
  "dependencyDashboard": false,
  "rebaseWhen": "auto",
  "baseBranches": ["master"],
  "enabledManagers": ["regex"],
  "regexManagers": [
    {
      "fileMatch": ["(^|/)Dockerfile[^/]*$"],
      "matchStrings": [
        "#\\srenovate:\\sdatasource=(?<datasource>.*?) depName=(?<depName>.*?)( versioning=(?<versioning>.*?))?\\s(ENV|ARG)?( extractVersion=(?<extractVersion>.*?))?\\s.*?_VERSION=(?<currentValue>.*)\\s"
       ]
    }
  ]
}
